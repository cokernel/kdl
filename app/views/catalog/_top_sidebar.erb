<div class="contact_us front_nav">
<ul><li><a href="<%= contact_us_catalog_path(:id => @document['id']) %>" 
class="lightboxLink">Contact us about this item</a></li></ul>
</div>
  <%- end -%>
<%- end -%>

<%= render 'catalog/calendar' %>

<%- if has_search_parameters? or @document -%>
