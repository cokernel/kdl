<div class="front_nav">
<ul>
<li><%= link_back_to_catalog %></li>
<li><%= link_to "Search history", search_history_index_path %></li>
<li><%= link_to "Start over", catalog_index_path %></li>
</ul>
</div>
  <%- end -%>
<%- end -%>
